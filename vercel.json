{
  "version": 2,
  "buildCommand": "pnpm nx build client",
  "outputDirectory": "dist/apps/client",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.internzvalley.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 